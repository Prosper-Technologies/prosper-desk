{
  "name": "prosper-desk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:up": "docker compose up -d postgres",
    "db:down": "docker compose down",
    "db:reset": "docker compose down && docker volume rm bluedesk_postgres_data && docker compose up -d postgres",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "dev:services": "docker compose up -d postgres && supabase start",
    "stop:services": "docker compose down && supabase stop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "supabase migration up --local",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "prod": "dotenv -e .env.production -- next dev",
    "dev": "next dev -p 4000",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
